<?xml version="1.0" encoding="utf-8"?>
<!--검색창 버튼 배경을 정의하는 xml-->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 흰색 배경 -->
    <solid android:color="#FFFFFF" />

    <!-- 둥근 모서리 -->
    <corners android:radius="20dp" />

    <!-- 안쪽 여백 (박스 테두리와 내용 사이) -->
    <padding
        android:left="12dp"
        android:right="12dp"
        android:top="8dp"
        android:bottom="8dp" />
</shape>
